<script type="text/javascript" src="/js/mootools-1.2.4.core-1.2.4.4-more.js"></script>

<script type="text/javascript" src="/js/iphone-checkboxes.js"></script>
<script type="text/javascript" src="/js/multibox/overlay.js"></script> 
<script type="text/javascript" src="/js/multibox/assets.js"></script> 
<script type="text/javascript" src="/js/multibox/multibox.js"></script> 

